{
  "name": "cleanly-server",
  "version": "0.0.1",
  "description": "server side for cleanly app",
  "main": "src/server.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": " node .",
    "start:dev": "NODE_ENV=development node ./node_modules/nodemon/bin/nodemon.js ."
  },
  "repository": "git@github.com:cleanlyer/cleanly-server.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "body-parser": "1.18.3",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-exception-handler": "1.0.3",
    "helmet": "3.12.2",
    "mongoose": "5.2.7",
    "morgan": "1.9.0",
    "multer": "1.3.1"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "faker": "4.1.0",
    "jest": "23.4.2",
    "nodemon": "1.18.3",
    "supertest": "3.1.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "testEnvironment": "node",
    "automock": false
  }
}
